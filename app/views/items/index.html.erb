<style>
    .first_food_items {
        min-height:300px;
        background-color:#FFF;
        border-radius:3px;
        border:1px solid #CDCDCD;
    }
    .first_food_items img{
        width:100%;
        border-bottom:1px solid #ddd;
        padding-bottom:5px;
        min-height:126px;
    }
    .first_food_items h3{
        margin:5px 0;
        font-size:15px;
        font-weight:500;
        font-family:Tahoma, Geneva, sans-serif;
        color:#606060;
        line-height:18px;
        height:23px;
    }
    .first_food_items h4{
        margin:30px 0;
        font-size:19px;
        font-weight:500;
        color:#FF6600;
        font-family:Tahoma, Geneva, sans-serif;

    }
    .first_food_items h4 span{
        margin:30px 0;
        font-size:12px;
        font-weight:900;
        color:#333333;
        text-decoration:line-through;
        font-family:Tahoma, Geneva, sans-serif;

    }
    .first_food_items_select_item {
        min-height:430px;
        border:1px solid #CDCDCD;
        background-color:#FFF;
    }
    .first_food_items_select_item_hader {
        height:50px;
        border-bottom:1px solid #ddd;
    }
    .first_food_items_select_item_hader  h3{
        font-size:17px;
        font-weight:900;
        color:#FF6600;
        margin:0;
        line-height:50px;
        font-family:Tahoma, Geneva, sans-serif;
    }
    .first_food_items_select_item_hader  span{
        font-size:17px;
        font-weight:900;
        color:#663300;
        margin:0 0 0 40px;
        line-height:50px;
        font-family:Tahoma, Geneva, sans-serif;
    }
    .first_food_items_select_item_selected  {
        width:100%;
        height:auto;
        margin:10px 0;
    }
    .first_food_items_select_item_selected .item_in_dec h4 {
        position:absolute;

    }
    .first_food_items_select_item_selected .item_in_dec h4 a i {
        font-size:26px;
    }
    .first_food_items_select_item_selected .item_in_dec h4 span {
        font-size:15px
    }
    .item_img img{
        width:100%;
        height:50px;
        border:1px solid #ddd;
        border-radius:3px;
        padding:2px;
    }
    .item_name h3{
        margin:5px 0;
        font-size:13px;
        margin-left:-20px;
        text-align:left;
        font-weight:bold;
    }
    .item_name h4{
        margin:3px 0;
        font-size:11px;
        margin-left:-20px;
        text-align:left;
        color:#666;
        font-weight:bold;
    }
    .item_total_price h3{
        margin:3px 0;
        font-size:13px;
        text-align:left;
        font-weight:bold;
        color:#5B5B5B;
    }
    .item_total_price h3 i{
        font-size:16px;
        text-align:right;
        color:#F00;
    }
    .money_receipt h2{
        font-size:18px;
        margin:0;
        font-weight:bold;
        font-family:"Palatino Linotype", "Book Antiqua", Palatino, serif;
    }
    .money_receipt h3{
        font-size:12px;
        margin:3px 0;
        font-weight:700;
        font-family:"Comic Sans MS", cursive;
    }
</style>
